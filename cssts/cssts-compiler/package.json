{
  "name": "cssts-compiler",
  "version": "0.2.4",
  "description": "CssTs compiler - parser, AST transformer, and type generator for CSS-in-TS",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run"
  },
  "keywords": [
    "cssts",
    "compiler",
    "parser",
    "generator",
    "css",
    "typescript"
  ],
  "license": "MIT",
  "dependencies": {
    "css-tree": "^3.1.0",
    "slime-ast": "^0.2.3",
    "slime-parser": "^0.2.3",
    "slime-token": "^0.2.3",
    "subhuti": "^0.2.3"
  },
  "devDependencies": {
    "slime-test": "^0.2.3",
    "tsdown": "^0.18.0",
    "typescript": "^5.8.3"
  },
  "gitHead": "4dbe04f31d770e413fa5a3eae426324e7af1e92c"
}
