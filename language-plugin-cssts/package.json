{
	"name": "language-plugin-cssts",
	"version": "0.2.87",
	"description": "Vue Language Plugin for CSSTS - provides LSP support for <script lang=\"cssts\"> in Vue SFC",
	"license": "MIT",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		}
	},
	"files": [
		"dist"
	],
	"sideEffects": false,
	"scripts": {
		"build": "tsdown",
		"dev": "tsdown --watch"
	},
	"dependencies": {
		"cssts-compiler": "^0.2.87"
	},
	"devDependencies": {
		"@types/node": "^22.10.4",
		"@vue/language-core": "^3.2.2",
		"tsdown": "^0.18.0",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"@vue/language-core": "^3.0.0"
	}
}