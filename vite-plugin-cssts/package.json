{
  "name": "vite-plugin-cssts",
  "version": "0.2.4",
  "description": "Vite plugin for CssTs - compile-time CSS class management",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "keywords": [
    "vite",
    "plugin",
    "css",
    "typescript",
    "atomic-css"
  ],
  "license": "MIT",
  "dependencies": {
    "cssts-ts": "^0.2.4",
    "cssts-compiler": "^0.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.2",
    "tsdown": "^0.18.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "gitHead": "4dbe04f31d770e413fa5a3eae426324e7af1e92c"
}
