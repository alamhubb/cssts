{
  "name": "csstsparent",
  "version": "0.2.86",
  "private": true,
  "type": "module",
  "description": "CSSTS - CSS-in-TypeScript Solution",
  "workspaces": [
    "cssts/*",
    "language-plugin-cssts",
    "vite-plugin-cssts",
